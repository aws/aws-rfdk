{
  "name": "aws-rfdk",
  "version": "0.42.0",
  "private": false,
  "description": "Package for core render farm constructs",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "stage-deadline": "bin/stage-deadline"
  },
  "jsii": {
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "aws-rfdk",
        "module": "aws_rfdk"
      }
    }
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com",
    "organization": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aws/aws-rfdk.git",
    "directory": "packages/aws-rfdk"
  },
  "pkglint": {
    "exclude": [
      "dependencies/cdk-point-dependencies",
      "ignore/strong-name-key",
      "package-info/homepage",
      "package-info/scripts/compat",
      "package-info/stability",
      "jsii/dotnet/icon-url",
      "jsii/dotnet/strong-name",
      "jsii/java",
      "jsii/dotnet"
    ]
  },
  "awslint": {
    "exclude": [
      "duration-prop-type:aws-rfdk.StaticPrivateIpServerProps.resourceSignalTimeout"
    ]
  },
  "scripts": {
    "awslint": "cdk-awslint",
    "build": "cdk-build",
    "build+test": "yarn run build && yarn test",
    "build+test+package": "yarn run build+test && yarn run package",
    "clean": "tsc --build --clean && bash ./clean.sh",
    "lint": "cdk-lint",
    "lintfix": "cdk-lint --fix",
    "package": "cdk-package",
    "pkglint": "pkglint -f",
    "test": "yarn testcdk && yarn testpy",
    "testcdk": "cdk-test",
    "testpy": "./test-python.sh",
    "watch": "cdk-watch"
  },
  "keywords": [
    "aws",
    "cdk",
    "rfdk",
    "rfdk-deadline",
    "deadline"
  ],
  "devDependencies": {
    "@types/aws-lambda": "^8.10.100",
    "@types/jest": "^27.5.2",
    "@types/sinon": "^10.0.11",
    "aws-sdk": "^2.1183.0",
    "aws-sdk-mock": "5.5.0",
    "cdk-build-tools": "0.42.0",
    "aws-cdk-lib": "2.33.0",
    "constructs": "^10.0.0",
    "dynalite": "^3.2.2",
    "pkglint": "0.42.0",
    "sinon": "^14.0.0"
  },
  "dependencies": {
    "aws-cdk-lib": "2.33.0",
    "constructs": "^10.0.0"
  },
  "peerDependencies": {
    "aws-cdk-lib": "2.33.0",
    "constructs": "^10.0.0"
  },
  "engines": {
    "node": ">= 14.15.0"
  },
  "stability": "stable",
  "maturity": "stable"
}
