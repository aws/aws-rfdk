  "resolutions": {
    "json5": "^2.2.2"
  },
  "devDependencies": {
    "@aws-sdk/client-ssm": "^3.296.0",
    "@aws-sdk/client-cloudformation": "^3.298.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.290.0",
    "@aws-sdk/client-secrets-manager": "^3.297.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^18.6.2",
    "aws-cdk-lib": "2.70.0",
    "aws-sdk": "^2.1334.0",
    "constructs": "^10.0.0",
    "fs-extra": "^11.1.0",
    "jest": "^27.5.1",
    "jest-junit": "^15.0.0",
    "lerna": "^6.5.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.5",
    "typescript": "~4.8.4"
  },
  "workspaces": {
    "packages": [
      "examples/**",
      "lambda-layers/",
