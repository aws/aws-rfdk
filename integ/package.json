    "@aws-cdk/aws-secretsmanager": "1.116.0",
    "@aws-cdk/core": "1.116.0",
    "aws-rfdk": "0.37.0",
    "aws-sdk": "^2.959.0"
  },
  "peerDependencies": {
    "@aws-cdk/aws-docdb": "1.116.0",
