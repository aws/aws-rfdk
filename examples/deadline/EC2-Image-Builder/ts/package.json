{
  "name": "all-in-farm-image-builder",
  "version": "0.25.0",
  "bin": {
    "app": "bin/app.js"
  },
  "config": {
    "deadline_ver": "10.1.13.1",
    "stage_path": "stage"
  },
  "scripts": {
    "build": "tsc",
    "build+test": "yarn build && yarn test",
    "cdk": "cdk",
    "clean": "bash ./clean.sh && tsc --build --clean",
    "stage": "stage-deadline ${npm_package_config_deadline_ver} --output ${npm_package_config_stage_path}",
    "test": "echo 'no tests to run'",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@types/node": "^14.14.14",
    "aws-cdk": "1.86.0",
    "ts-node": "^9.1.1",
    "typescript": "~4.1.3"
  },
  "dependencies": {
    "@aws-cdk/aws-iam": "1.86.0",
    "@aws-cdk/aws-imagebuilder": "1.86.0",
    "@aws-cdk/aws-ec2": "1.86.0",
    "@aws-cdk/aws-s3-assets": "1.86.0",
    "@aws-cdk/core": "1.86.0",
    "aws-rfdk": "0.25.0"
  }
}
