{
  "name": "all-in-farm-basic",
  "version": "0.20.0",
  "bin": {
    "app": "bin/app.js"
  },
  "config": {
    "deadline_ver": "10.1.11.5",
    "stage_path": "stage"
  },
  "scripts": {
