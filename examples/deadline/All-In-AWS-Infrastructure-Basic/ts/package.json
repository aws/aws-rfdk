    "watch": "tsc -w"
  },
  "devDependencies": {
    "@types/node": "^14.14.6",
    "aws-cdk": "1.72.0",
    "ts-node": "^9.0.0",
    "typescript": "~4.0.5"
  },
  "dependencies": {
    "@aws-cdk/core": "1.72.0",
    "aws-rfdk": "0.20.0",
    "source-map-support": "^0.5.19"
  }
}
